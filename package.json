  "description": "The authentic genarator for random data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ts-node": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch '*.ts' --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
