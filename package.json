    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "babel-jest": "^27.3.1",
    "cross-env": "7.0.3",
    "jest": "^27.3.1",
    "nodemon": "2.0.13",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "^4.4.3"
  }
